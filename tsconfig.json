{
  "compileOnSave": false,
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@myworkspacename/my-lib-one": [
        "libs/my-lib-one/src/index.ts"
      ],
      "@myworkspacename/my-lib-one-with-workspace-schematic": [
        "libs/my-lib-one-with-workspace-schematic/src/index.ts"
      ],
      "@myworkspacename/my-lib-two-with-workspace-schematic": [
        "libs/my-lib-two-with-workspace-schematic/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
